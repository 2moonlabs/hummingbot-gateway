{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Ethereum Config",
  "description": "A schema for ethereum-specific configuration parameters",
  "type": "object",
  "properties": {
    "gasLimitTransaction": {
      "description": "Gas limit for transactions",
      "type": "integer"
    },
    "manualGasPrice": {
      "description": "Manual gas price",
      "type": "integer"
    },
    "retryInterval": {
      "description": "Retry interval in seconds",
      "type": "integer"
    },
    "retryFeeMultiplier": {
      "description": "Multiplier for retry fee",
      "type": "number"
    }
  },
  "required": ["gasLimitTransaction", "manualGasPrice"]
}

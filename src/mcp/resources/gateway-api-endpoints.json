        "category": "status"
      },
      {
        "method": "GET",
        "path": "/chains/{chain}/balances",
        "description": "Get wallet balances for tokens",
        "category": "wallet"
      },
      {
        "method": "POST",
        "path": "/chains/ethereum/approve",
        "description": "Approve token spending allowance",
        "category": "trading"
      },
      {
        "method": "GET",
        "path": "/chains/ethereum/allowances",
        "description": "Check token spending allowances",
        "category": "trading"
      },
      {

      },
      {
        "method": "POST",
        "path": "/chains/{chain}/estimate-gas",
        "description": "Estimate gas costs for transactions",
        "category": "trading"
      },
      {
        "method": "POST",
        "path": "/chains/{chain}/poll",
        "description": "Poll transaction status",
        "category": "status"
      }
    ]
  },

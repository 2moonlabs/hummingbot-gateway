{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "allowedSlippage": { "type": "string" },
      "contractAddresses": {
        "type": "object",
        "patternProperties": {
          "^\\w+$": {
            "type": "object",
            "properties": {
              "whirlpoolsConfig": { "type": "string" }
            },
            "required": ["whirlpoolsConfig"],
            "additionalProperties": false
          }
        },
        "additionalProperties": false
      }
    },
    "additionalProperties": false,
    "required": ["allowedSlippage", "contractAddresses"]
  }
  
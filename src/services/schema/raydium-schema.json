          "defaultPools": {
            "type": "object",
            "additionalProperties": false
          },
          "pools": {
            "type": "object",
            "patternProperties": {
              "^[A-Z]+-[A-Z]+$": {
                "type": "string"
              }
            },
            "additionalProperties": false
          }
        },
        "required": ["allowedSlippage", "defaultPools", "pools"],
        "additionalProperties": false
